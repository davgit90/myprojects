## CICD with kubernetes
pipeline{
    agent any
    environment{
        DOCKER_TAG = getDockerTag()
    }
    stages{
        stage("Build docker image"){
            steps{
                sh " docker build . -t aditya/nodeapp:${DOCKER_TAG} "
            }
        }
    }
      
}
## get latest tag
def getDockerTag(){
    def tag =sh script: 'git rev-parse HEAD', returnStdout: true
    return tag
}   
